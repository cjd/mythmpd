####### Install

install_target: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/lib/mythtv/plugins/ || $(MKDIR) $(INSTALL_ROOT)/usr/lib/mythtv/plugins/ 
	-$(INSTALL_PROGRAM) "$(TARGET)" "$(INSTALL_ROOT)/usr/lib/mythtv/plugins/$(TARGET)"

uninstall_target:  FORCE
	-$(DEL_FILE) "$(INSTALL_ROOT)/usr/lib/mythtv/plugins/$(TARGET)"
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/lib/mythtv/plugins/ 


install_uifiles: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/usr/share/mythtv/themes/default/ || $(MKDIR) $(INSTALL_ROOT)/usr/share/mythtv/themes/default/ 
	-$(INSTALL_FILE) /home/caysho/mythplugins/mythmpd/mythmpd/mythmpd-ui.xml $(INSTALL_ROOT)/usr/share/mythtv/themes/default/


uninstall_uifiles:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/usr/share/mythtv/themes/default/mythmpd-ui.xml
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/share/mythtv/themes/default/ 


install:  install_target install_uifiles  FORCE

uninstall: uninstall_target uninstall_uifiles   FORCE

FORCE:

